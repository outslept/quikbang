{
  "version": 2,
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "devCommand": "pnpm dev:server",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/data/:path*",
      "destination": "/data/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
